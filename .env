# .env file

# --- General Settings ---
PROJECT_NAME="YL Competitions MVP"
ENVIRONMENT=local
SECRET_KEY=YOUR_VERY_SECRET_AND_UNIQUE_KEY_PLEASE_CHANGE_ME # Сгенерируй новый `openssl rand -hex 32`

# --- Frontend ---
FRONTEND_HOST=http://localhost:5173

# --- CORS ---
BACKEND_CORS_ORIGINS=http://localhost:5173,https://your-staging-domain.com

# --- Database (SQLite) ---
SQLITE_DB_FILE=users.db

# --- Telegram ---
TELEGRAM_BOT_TOKEN="PASTE_YOUR_BOT_TOKEN_HERE" # <<< ВАЖНО: Вставь свой токен
TELEGRAM_BOT_API_KEY="PASTE_YOUR_GENERATED_API_KEY_HERE" # <<< ВАЖНО: Сгенерируй ключ (например, `openssl rand -hex 32`)
TELEGRAM_CLIENT_ID="YOUR_TELEGRAM_CLIENT_ID_HERE" # <<< ВАЖНО: Вставь ID OAuth приложения
TELEGRAM_CLIENT_SECRET="YOUR_TELEGRAM_CLIENT_SECRET_HERE" # <<< ВАЖНО: Вставь секрет OAuth приложения

# --- Sentry (Optional) ---
# SENTRY_DSN=YOUR_SENTRY_DSN